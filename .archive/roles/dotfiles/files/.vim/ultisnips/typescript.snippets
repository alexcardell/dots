snippet exp "Jest - expect"
expect(${1:expected}).${2:toBe(${3:received})};
endsnippet

snippet desc "Jest - describe"
describe('$1', () => {
	${VISUAL}$0
});
endsnippet

snippet test "Jest - test"
test('$1', () => {
	const expected = ${2:1};
	const received = ${3:1};
});
endsnippet

snippet fn "function (arrow)"
const ${1:fn} = (${2}) => {
	${0:${VISUAL:implementation}}
};
endsnippet

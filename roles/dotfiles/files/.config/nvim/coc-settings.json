{
  "codeLens.enable": true,
  "languageserver": {
     "reason": {
      "command": "reason-language-server",
      "filetypes": ["reason"]
     },
     "scala": {
      "command": "metals-vim",
      "filetypes": ["scala", "sbt"],
      "rootPatterns": ["build.sbt"]
     },
     "golang": {
      "command": "gopls",
      "filetypes": ["go"],
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"]
    }
  },
  "java.errors.incompleteClasspath.severity": "ignore"
}

{
  "codeLens.enable": true,
  "languageserver": {
     "reason": {
      "command": "reason-language-server",
      "filetypes": ["reason"]
     },
     "scala": {
      "command": "metals-vim",
      "filetypes": ["scala", "sbt"],
      "rootPatterns": ["build.sbt"]
     }
  },
  "java.errors.incompleteClasspath.severity": "ignore",
  "python.jediEnabled": false
}

#include "/home/alex/.Xresources.d/base16-xresources/base16-tomorrow-night-256.Xresources"

Xft.hinting: true
Xft.antialias: true

*.font:             xft:Sauce Code Pro Nerd Font:size=18
*.boldFont:         xft:Sauce Code Pro Nerd Font:bold:size=18
*.italicFont:       xft:Sauce Code Pro Nerd Font:italic:size=18
*.boldItalicFont:   xft:Sauce Code Pro Nerd Font:bold:italic:size=18

! Scrolling
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true
URxvt.scrollBar:    false

URxvt.termName: rxvt-unicode-256color

!------------------------------------------------------------------------------
! ROFI Color theme
!------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.terminal: urxvt
rofi.fuzzy: true
rofi.kb-cancel: Escape,Control+c
rofi.hide-scrollbar: true
rofi.fake-transparency: true

! ! Property Name     BG       Border   Separator
! rofi.color-window:  base00, base02, base01
